{
  "name": "hakathon_iu5_2025_frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"tailwindcss -i ./app/globals-src.css -o ./app/globals.css --watch\" \"next dev\"",
    "build": "tailwindcss -i ./app/globals-src.css -o ./app/globals.css --minify && next build",
    "build:css": "tailwindcss -i ./app/globals-src.css -o ./app/globals.css --minify",
    "watch:css": "tailwindcss -i ./app/globals-src.css -o ./app/globals.css --watch",
    "start": "next start",
    "clean": "rm -rf .next node_modules package-lock.json && npm install",
    "clean:win": "rmdir /s /q .next && rmdir /s /q node_modules && del package-lock.json && npm install",
    "lint": "eslint",
    "prettier:fix": "prettier --write .",
    "eslint:fix": "eslint . --ext .ts,.tsx --fix .",
    "ts:check": "tsc --noEmit"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.5",
    "axios": "^1.12.2",
    "next": "16.0.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "socket.io-client": "^4.8.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "concurrently": "^8.2.2",
    "eslint": "^9",
    "eslint-config-next": "16.0.0",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.18",
    "typescript": "^5"
  }
}
